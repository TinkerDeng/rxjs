{
  "restartable": "rs",
  "ignore": [
      ".git",
      ".svn"
  ],
  "events": {
    "start": "cls || clear"
  },
  "verbose": true,
  "execMap": {
      "js": "node --harmony"
  },
  "env": {
      "NODE_ENV": "development"
  },
  "ext": "js json",
  "ignoreRoot": [".git", ".jpg", ".whatever"]
}